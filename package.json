{
  "name": "ai-town",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:backend dev:frontend",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "predev": "convex dev --run init --until-success",
    "dev:backend": "convex dev --tail-logs",
    "dev:frontend": "vite",
    "predev:e2e": "CONVEX_AGENT_MODE=anonymous convex dev --run init --until-success --local --local-cloud-port ${E2E_CONVEX_PORT:-3212} --local-site-port ${E2E_CONVEX_SITE_PORT:-3213}",
    "dev:backend:e2e": "CONVEX_AGENT_MODE=anonymous convex dev --tail-logs --local --local-cloud-port ${E2E_CONVEX_PORT:-3212} --local-site-port ${E2E_CONVEX_SITE_PORT:-3213}",
    "dev:frontend:e2e": "VITE_E2E=1 VITE_CONVEX_URL=http://127.0.0.1:${E2E_CONVEX_PORT:-3212} vite --mode e2e --port 4173 --strictPort",
    "dashboard": "convex dashboard",
    "dev:e2e": "npm-run-all --parallel dev:backend:e2e dev:frontend:e2e",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --verbose",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "mcp:werewolf": "node --loader ts-node/esm mcp/werewolf/server.ts"
  },
  "dependencies": {
    "@clerk/clerk-react": "^4.27.0",
    "@elizaos/core": "^1.7.2",
    "@modelcontextprotocol/sdk": "^1.8.0",
    "@pixi/react": "^7.1.0",
    "@pixi/sound": "^5.2.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "clsx": "^2.0.0",
    "convex": "^1.19.2",
    "dotenv": "^16.1.4",
    "eslint": "8.42.0",
    "hnswlib-node": "^1.4.2",
    "pixi-viewport": "^5.0.1",
    "pixi.js": "^7.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-github-btn": "^1.4.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^7.12.0",
    "react-toastify": "^9.1.3",
    "replicate": "^0.18.0",
    "typescript": "5.1.3",
    "update-browserslist-db": "^1.1.2",
    "uplot": "^1.6.25",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.2.14",
    "@playwright/test": "^1.57.0",
    "@types/css-font-loading-module": "^0.0.8",
    "@types/jest": "^29.5.4",
    "@types/react-modal": "^3.16.0",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "canvas": "^3.2.1",
    "concurrently": "^8.2.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.28",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "vite": "^4.4.9"
  },
  "type": "module"
}
