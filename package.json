{
  "name": "ai-town",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "node scripts/patch-fast-redact.cjs",
    "dev": "npm-run-all --parallel dev:backend dev:frontend",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "predev": "convex dev --run init --until-success",
    "dev:backend": "convex dev --tail-logs",
    "dev:frontend": "vite",
    "dashboard": "convex dashboard",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --verbose",
    "test:e2e": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.e2e.config.ts --runInBand --verbose",
    "test:e2e:world": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.e2e.config.ts --runInBand --verbose tests/e2e/world.e2e.test.ts",
    "test:e2e:agent": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.e2e.config.ts --runInBand --verbose tests/e2e/agent.e2e.test.ts",
    "test:e2e:conversation": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.e2e.config.ts --runInBand --verbose tests/e2e/conversation.e2e.test.ts",
    "test:e2e:full": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.e2e.config.ts --runInBand --verbose tests/e2e/full-game.e2e.test.ts",
    "test:e2e:infra": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.e2e.config.ts --runInBand --verbose tests/e2e/infrastructure.e2e.test.ts",
    "test:e2e:eliza": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.e2e.config.ts --runInBand --verbose tests/e2e/eliza-runtime.e2e.test.ts",
    "test:e2e:eliza-integration": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.e2e.config.ts --runInBand --verbose tests/e2e/eliza-integration.e2e.test.ts",
    "test:e2e:actions": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.e2e.config.ts --runInBand --verbose tests/e2e/eliza-actions.e2e.test.ts",
    "test:e2e:startup": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.e2e.config.ts --runInBand --verbose tests/e2e/startup.e2e.test.ts",
    "cleanup:test-agents": "npx ts-node --esm scripts/cleanup-test-agents.ts",
    "knip": "knip"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.0.0",
    "@clerk/clerk-react": "^4.27.0",
    "@elizaos/core": "2.0.0-alpha.2",
    "@elizaos/plugin-inmemorydb": "^2.0.0-alpha.2",
    "@elizaos/plugin-openai": "2.0.0-alpha.2",
    "@pixi/react": "^7.1.0",
    "@pixi/sound": "^5.2.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "ai": "^6.0.44",
    "clsx": "^2.0.0",
    "convex": "^1.19.2",
    "dotenv": "^16.1.4",
    "eslint": "8.42.0",
    "pixi-viewport": "^5.0.1",
    "pixi.js": "^7.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^7.12.0",
    "react-toastify": "^9.1.3",
    "replicate": "^0.18.0",
    "typescript": "5.1.3",
    "uplot": "^1.6.25",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@types/css-font-loading-module": "^0.0.8",
    "@types/jest": "^29.5.4",
    "@types/react-modal": "^3.16.0",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "canvas": "^3.2.1",
    "jest": "^29.7.0",
    "knip": "^5.82.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.28",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "vite": "^4.4.9"
  },
  "type": "module"
}